    "serve": "node_modules\\.bin\\ng serve %npm_package_config_app% --aot=true --progress=false --base-href=/",
    "build-dsl": "npm run build --repo:app=DailySpendLimit",
    "ngc-dsl": "npm run ngm --repo:app=DailySpendLimit",
    "ngm-dsl": "npm run ngm --repo:app=DailySpendLimit",
    "ngs-dsl": "npm run ngs --repo:app=DailySpendLimit",
    "serve-dsl": "npm run serve --repo:app=DailySpendLimit"
