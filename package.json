{
  "name": "repo",
  "version": "1.0.0",
  "scripts": {
    "build-lib": "ng build core && ng build gapi && ng build material-helpers && ng build material-cms",
    "ngc-cms": "ng g component --project=material-cms --",
    "ngc-dsl": "ng g component --project=DailySpendLimit --",
    "ngc-vtw": "ng g component --project=VinesToWinesDM --",
    "ngm-dsl": "ng g module --project=DailySpendLimit --",
    "ngm-vtw": "ng g module --project=VinesToWinesDM --",
    "serve-dsl": "ng s DailySpendLimit --aot=true --progress=false --base-href=/",
    "serve-sc": "ng s StavesChurch --aot=true --progress=false --base-href=/",
    "serve-tk": "ng s TiffanysKitchen --aot=true --progress=false --base-href=/",
    "serve-vtw": "ng s VinesToWinesDM --aot=true --progress=false --base-href=/",
    "build-dsl": "ng b DailySpendLimit --prod --noprogress --output-path=../DailySpendLimit/docs/ --base-href=/DailySpendLimit/ && copy ..\\DailySpendLimit\\docs\\index.html ..\\DailySpendLimit\\docs\\404.html",
    "build-sc": "ng b StavesChurch --prod --noprogress --output-path=../StavesChurch/docs/ --base-href=/StavesChurch/ && copy ..\\StavesChurch\\docs\\index.html ..\\StavesChurch\\docs\\404.html",
    "build-tk": "ng b TiffanysKitchen --prod --noprogress --output-path=../TiffanysKitchen/docs/ --base-href=/TiffanysKitchen/ && copy ..\\TiffanysKitchen\\docs\\index.html ..\\TiffanysKitchen\\docs\\404.html",
    "build-vtw": "ng b VinesToWinesDM --prod --noprogress --output-path=../VinesToWinesDM/docs/ --base-href=/VinesToWinesDM/ && copy ..\\VinesToWinesDM\\docs\\index.html ..\\VinesToWinesDM\\docs\\404.html"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.0.0",
    "@angular/cdk": "~7.0.0",
    "@angular/common": "~7.0.0",
    "@angular/compiler": "~7.0.0",
    "@angular/core": "~7.0.0",
    "@angular/flex-layout": "github:angular/flex-layout-builds",
    "@angular/forms": "~7.0.0",
    "@angular/http": "~7.0.0",
    "@angular/material": "~7.0.0",
    "@angular/platform-browser": "~7.0.0",
    "@angular/platform-browser-dynamic": "~7.0.0",
    "@angular/pwa": "^0.10.4",
    "@angular/router": "~7.0.0",
    "@angular/service-worker": "~7.0.0",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "jsrsasign": "^8.0.12",
    "material-icon-font": "^3.0.1",
    "roboto-fontface": "^0.10.0",
    "rxjs": "~6.3.3",
    "typeface-great-vibes": "0.0.54",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular-devkit/build-ng-packagr": "~0.10.0",
    "@angular/cli": "~7.0.6",
    "@angular/compiler-cli": "~7.0.0",
    "@angular/language-service": "~7.0.0",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "ng-packagr": "^4.2.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6"
  }
}
