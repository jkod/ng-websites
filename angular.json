{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "core": {
      "root": "projects/core",
      "sourceRoot": "projects/core/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/core/tsconfig.lib.json",
            "project": "projects/core/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/core/src/test.ts",
            "tsConfig": "projects/core/tsconfig.spec.json",
            "karmaConfig": "projects/core/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/core/tsconfig.lib.json",
              "projects/core/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "TiffanysKitchen": {
      "root": "projects/TiffanysKitchen/",
      "sourceRoot": "projects/TiffanysKitchen/src",
      "projectType": "application",
      "prefix": "tf",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss",
          "spec": false
        },
        "@schematics/angular:class": {
          "spec": false
        },
        "@schematics/angular:directive": {
          "spec": false
        },
        "@schematics/angular:guard": {
          "spec": false
        },
        "@schematics/angular:module": {
          "spec": false
        },
        "@schematics/angular:pipe": {
          "spec": false
        },
        "@schematics/angular:service": {
          "spec": false
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/TiffanysKitchen",
            "index": "projects/TiffanysKitchen/src/index.html",
            "main": "projects/TiffanysKitchen/src/main.ts",
            "polyfills": "projects/TiffanysKitchen/src/polyfills.ts",
            "tsConfig": "projects/TiffanysKitchen/tsconfig.app.json",
            "assets": [
              "projects/TiffanysKitchen/src/assets",
              "projects/TiffanysKitchen/src/manifest.json"
            ],
            "styles": [
              "./node_modules/typeface-great-vibes/index.css",
              "./node_modules/roboto-fontface/css/roboto/sass/roboto-fontface.scss",
              "./node_modules/material-icon-font/iconfont/material-icons.css",
              "projects/TiffanysKitchen/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/TiffanysKitchen/src/environments/environment.ts",
                  "with": "projects/TiffanysKitchen/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ],
              "serviceWorker": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "TiffanysKitchen:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "TiffanysKitchen:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "TiffanysKitchen:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/TiffanysKitchen/src/test.ts",
            "polyfills": "projects/TiffanysKitchen/src/polyfills.ts",
            "tsConfig": "projects/TiffanysKitchen/tsconfig.spec.json",
            "karmaConfig": "projects/TiffanysKitchen/karma.conf.js",
            "styles": [
              "projects/TiffanysKitchen/src/styles.scss"
            ],
            "scripts": [],
            "assets": [
              "projects/TiffanysKitchen/src/assets",
              "projects/TiffanysKitchen/src/manifest.json"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/TiffanysKitchen/tsconfig.app.json",
              "projects/TiffanysKitchen/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
